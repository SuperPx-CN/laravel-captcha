{
	"name": "superpx-cn/laravel-captcha",
	"type": "package",
	"description": "Laravel Captcha Package",
	"keywords": ["laravel Captcha", "laravel Security", "laravel Captcha", "laravel Security", "Captcha"],
	"homepage": "https://github.com/SuperPx-CN/laravel-captcha",
	"license": "MIT",
	"authors": [
        {
            "name": "SuperPx",
            "email": "160484842@qq.com",
            "homepage": "https://github.com/SuperPx-CN",
            "role": "Developer"
        },
		{
			"name": "Muharrem ERÄ°N",
			"email": "me@mewebstudio.com",
			"homepage": "https://github.com/mewebstudio",
			"role": "Developer"
		}
	],
	"require": {
		"php": "^7.2|^8.0",
		"ext-gd": "*",
		"illuminate/config": "~5|^6|^7|^8|^9",
		"illuminate/filesystem": "~5|^6|^7|^8|^9",
		"illuminate/support": "~5|^6|^7|^8|^9",
		"illuminate/hashing": "~5|^6|^7|^8|^9",
		"illuminate/session": "~5|^6|^7|^8|^9",
		"intervention/image": "~2.5"
	},
	"require-dev": {
		"phpunit/phpunit": "^8.5|^9.0",
		"mockery/mockery": "^1.0"
	},
	"autoload": {
		"psr-4": {
			"Mews\\Captcha\\": "src/"
		},
		"files": [
			"src/helpers.php"
		]
	},
	"autoload-dev": {
		"classmap": [
			"tests"
		],
		"psr-4": {
			"Mews\\Test\\": "tests/"
		}
	},
	"minimum-stability": "stable",
	"extra": {
		"laravel": {
			"providers": [
				"Mews\\Captcha\\CaptchaServiceProvider"
			],
			"aliases": {
				"Captcha": "Mews\\Captcha\\Facades\\Captcha"
			}
		}
	}
}
